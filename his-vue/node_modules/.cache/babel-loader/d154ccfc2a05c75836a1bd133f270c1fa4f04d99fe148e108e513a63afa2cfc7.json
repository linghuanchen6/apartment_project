\" width=\"180\">\r\n          </el-table-column>\r\n  \r\n          <el-table-column prop=\"phone\" label=\"手机号\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"updateTime\" label=\"最后操作时间\">\r\n          </el-table-column>\r\n  \r\n          <el-table-column label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-edit\" @click=\"modify(scope.row.username)\" ></el-button>\r\n              <el-button type=\"danger\" size=\"mini\" icon=\"el-icon-delete\" @click=\"remove(scope.row.username,scope.row.name)\"></el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n  \r\n  \r\n        <el-pagination\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"queryCondition.currentPage\"\r\n            :page-sizes=\"[1, 4, 5, 10]\"\r\n            :page-size=\"queryCondition.pageSize\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"employPage.total\">\r\n        </el-pagination>\r\n  \r\n      </el-card>\r\n      <el-dialog :title=\"actionType\" :visible.sync=\"OpenDialog\" width=\"30%\" center>\r\n        <el-form :model=\"formData\" :rules=\"formRules\" ref=\"formRef\">\r\n          <el-form-item label=\"员工姓名\" label-width=\"100px\" :label-width=\"name\">\r\n            <el-input v-model=\"formData.name\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"账号\" label-width=\"100px\" :label-width=\"username\">\r\n            <el-input v-model.number=\"formData.username\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"手机号\" label-width=\"100px\" :label-width=\"drugPrice\">\r\n            <el-input v-model.number=\"formData.phone\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"性别\" label-width=\"100px\" :label-width=\"drugType\">\r\n  